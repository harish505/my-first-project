pipeline{
    agent{
        docker{
            image 'abhishekf5/maven-abhishek-docker-agent:v1'
            args '--user root -v /var/run/docker.sock:/var/run/docker.sock'
        }
    }
    stages{
        stage('checkout scm'){
            steps{
                git("url :https://github.com/harish505/my-first-project.git")
            }
        }
        stage('build'){
            steps{
                sh "mvn clean package"
            }
        }
    }
}
